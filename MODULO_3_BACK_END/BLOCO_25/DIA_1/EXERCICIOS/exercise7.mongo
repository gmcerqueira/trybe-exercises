db.clientes
  .aggregate([
    {
      $group: {
        _id: { sexo: "$sexo", uf: "$endereco.uf" },
        totalClientesSexo: { $sum: 1 },
      },
    },
    { $sort: { "_id.uf": 1, "_id.sexo": 1 } },
    {$project:{
      _id:0,
      estado:"$_id.uf",
      sexo:"$_id.sexo",
      total:"$totalClientesSexo",
    }}
  ])
  .pretty();
