db.vendas
  .aggregate([
    {
      $match: {
        status: { $in: ["ENTREGUE", "EM SEPARACAO"] },
      },
    },
    {
      $group: {
        _id: "$clienteId",
        comprasRealizadas: {
          $sum: 1,
        },
      },
    },
    {
      $match: {
        comprasRealizadas: { $gt: 5 },
      },
    },
    { $count: "clientes" }
  ])
  .pretty();
